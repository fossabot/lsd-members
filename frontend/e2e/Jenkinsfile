node {

  stage('Checkout') {
    git credentialsId: 'github-lsd-members',
        url: 'git@github.com:jSherz/lsd-members.git'
  }

  dir('frontend') {
    stage('Install') {
      sh 'yarn install'
    }

    stage('e2e') {
      ansiColor('css') {
        sh 'yarn run e2e-ci'
      }
    }
  }

}
